--
-- Index de la taula `keywords`
--
ALTER TABLE `keywords`
  ADD PRIMARY KEY (`idKeyword`);

--
-- Index de la taula `keywordsnoticia`
--
ALTER TABLE `keywordsnoticia`
  ADD PRIMARY KEY (`idnoticia`,`idkeyword`),
  ADD KEY `fk_keywordsNoticia_1_idx` (`idkeyword`);

--
-- Index de la taula `noticias`
--
ALTER TABLE `noticias`
  ADD PRIMARY KEY (`idnoticia`),
  ADD KEY `fk_noticias_1_idx` (`idseccion`),
  ADD KEY `fk_noticias_2_idx` (`autor`),
  ADD KEY `fk_noticias_3_idx` (`editor`);

--
-- Index de la taula `seccion`
--
ALTER TABLE `seccion`
  ADD PRIMARY KEY (`idseccion`);

